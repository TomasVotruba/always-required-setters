{
    "name": "tomasvotruba/ctor",
    "type": "phpstan-extension",
    "description": "Prefer constructor over always called setters",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "phpstan/phpstan": "^2.1.14",
        "webmozart/assert": "^1.11"
    },
    "autoload": {
        "psr-4": {
            "TomasVotruba\\Ctor\\": "src"
        }
    },
    "extra": {
        "phpstan": {
            "includes": [
                "config/extension.neon"
            ]
        }
    }
}
